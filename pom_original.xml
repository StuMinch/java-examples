    <repositories>
        <repository>
            <id>public</id>
            <url>http://vault.cnvrmedia.net/nexus/content/groups/public/</url>
        </repository>
        <repository>
            <id>confluent</id>
            <url>http://packages.confluent.io/maven/</url>
        </repository>
    </repositories>

    <properties>
        <kafka.version>2.4.0</kafka.version>
        <kafka.scala.version>2.12</kafka.scala.version>
        <confluent.version>5.4.0</confluent.version>
        <avro.version>1.7.7</avro.version>
        <selenium.version>4.0.0</selenium.version>

        <!-- Base URLs-->
        <issue.base.url>http://jira.cnvrmedia.net</issue.base.url>
        <scm.base.url>scm:git:http://stash.cnvrmedia.net/scm</scm.base.url>
        <scm.view.base.url>http://stash.cnvrmedia.net/projects</scm.view.base.url>
        <ci.base.url>http://builds.cnvrmedia.net/browse</ci.base.url>
        <maven.compiler.source>11</maven.compiler.source>
        <maven.compiler.target>11</maven.compiler.target>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
    </properties>

